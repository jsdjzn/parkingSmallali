.customerbtn{
  width: 100%;
  height: 90rpx;
  padding-right: 32rpx;
  text-align: left;
}
#one,#two,#four{
  margin-top: 30rpx
}
.dyt-list {
  margin-top: 0;
}
.dyt-list .am-list-item-thumb {
  border-radius: 5px;
}
.dyt-list .am-list-brief {
  color: #909090;
}

.dyt-list .am-list-extra {
  color: #000;
}

.am-list-sticky {
  position: sticky;
  top: 0;
  background-color: #fff;
  z-index: 2;
}